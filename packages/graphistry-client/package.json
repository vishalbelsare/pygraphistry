{
  "name": "@graphistry/graphistry-client",
  "version": "2.10.0",
  "description": "Clientside API for interacting with a Graphistry embedding",
  "main": "lib/index.js",
  "scripts": {
    "clean:docs": "rm -rf docs",
    "docs": "jsdoc -c conf.json -R ./README.md -d docs",
    "build-es5": "babel src --out-dir lib",
    "build-include": "cross-env NODE_ENV=production node ./tool/build-resource -- -- 2",
    "build": "npm run build-es5 && npm run build-include",
    "predocs": "npm run clean:docs",
    "preserve:docs": "npm run docs",
    "serve:docs": "http-server -o -p 9000 -c-1 ./docs"
  },
  "author": "",
  "license": "ISC",
